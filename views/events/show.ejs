<div class = "uk-grid">
	
	<div class = "uk-width-1-1">
		<h3 class= "available-times-header"> Available Times </h3>
	</div>

	<% if (availableTimes.length > 0){%>
		<% for(var i = 0; i < availableTimes.length; i ++ ){ %>
				<form class = "uk-form", action="/events/<%= date %>", method="POST">
					<div class = "uk-form-row">
						<label><%= availableTimes[i].start_time + " - " + availableTimes[i].end_time%> </label>
						<input type="hidden", value = "<%= availableTimes[i].appointment_id %>">
						<input type="submit", class="uk-button uk-button-success" value = "Book this appointment">
					</div>
				</form>
		<% }%>
	<% } else { %>

	<div class= "uk-width-1-1">
		<h1> No Available Times </h1>
	</div>
	<% } %>
</div>

</div>
